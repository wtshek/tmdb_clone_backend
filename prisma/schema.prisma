// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Asset {
   id Int @id @default(autoincrement())
   title String
   tmdb_id String
   media_type String
   image String
   time_window String
   available String
}

